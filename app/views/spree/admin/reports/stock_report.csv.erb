<%
   options = { :row_sep => nil}
   Encoding.default_external = "UTF-8"
   headers = [Spree.t(:product_name),Spree.t(:variant_name),Spree.t(:sku),Spree.t(:price),Spree.t(:stock_items_count)]
%>
<%= CSV.generate_line headers,options %>
<% @variants_before_paginate.each do |variant| %>
  <%= CSV.generate_line(
        [variant.product.name,variant.options_text,variant.sku,variant.display_price,variant.total_on_hand],
        options
      ).html_safe
    %>
<% end %>